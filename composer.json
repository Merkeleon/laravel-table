{
  "name": "merkeleon/laravel-table",
  "description": "Table functionality for Laravel collections",
  "keywords": ["laravel","table","html","model","sorting","filtering", "merkeleon"],
  "license": "MIT",
  "authors": [
    {
      "name": "codeator",
      "email": "codeator@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "laravel/framework": "~5.6",
    "doctrine/dbal": "~2.3",
    "maatwebsite/excel": "^3.0",
    "merkeleon/laravel-elastic-reader": "dev-master",
    "opis/closure": "^3.1"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Merkeleon\\Table\\Providers\\TableServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Merkeleon\\Table\\": "src/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
